\t\t\twindow.onresize = reportWindowSize;\n\t\t\tvar waitForProgressBar9559_6c9af0b4 = setInterval(function () {\n\t\t\t\tif (typeof ProgressBar !== \"undefined\" ) {\n\t\t\t\t\tclearInterval(waitForProgressBar9559_6c9af0b4);\n\t\t\t\t\tlet responsiveStrokeSizes = [2,2,2];\n\t\t\t\t\tlet initialStroke;\n\t\t\t\t    if( window.innerWidth < 768 ) {\n\t\t\t\t        initialStroke = responsiveStrokeSizes[2];\n\t\t\t        } else if( window.innerWidth < 1025 ) {\n                        initialStroke = responsiveStrokeSizes[1];\n\t\t\t\t    } else {\n                        initialStroke = responsiveStrokeSizes[0];\n                    }\n\t\t\t\t\tlet progressBar9559_6c9af0b4 = new ProgressBar.Line(\"#kb-progress-bar9559_6c9af0-b4\", {\n\t\t\t\t\t\tcolor: \"var(--global-palette1, #2B6CB0)\",\n\t\t\t\t\t\ttrailColor: \"var(--global-palette7, #EDF2F7)\",\n\t\t\t\t\t\tduration: \"2000\",\n\t\t\t\t\t\teasing: \"easeInOut\",\n\t\t\t\t\t\tstrokeWidth: initialStroke,\n\t\t\t\t\t});\n\t\t\t\tlet progressBarController9559_6c9af0b4 = new ScrollMagic.Controller();\n\t\t\t\tlet desiredAnimation = new ScrollMagic.Scene({triggerElement: \"#kb-progress-bar9559_6c9af0-b4\"});\n\t\t\t\tdesiredAnimation.triggerHook(0.88);\n\t\t\t\tdesiredAnimation.addTo( progressBarController9559_6c9af0b4 );\n\t\t\t\tdesiredAnimation.on(\"start\", function (e) {progressBar9559_6c9af0b4.animate(\n\t\t\t\t\t\t\t0.8 ,\n\t\t\t\t            {\n\t\t\t\t\t\t\t\t duration: 2000,\n\t                             step: function(state, bar) {\n\t                                let value = 0;\n                                    let elementContainer = document.getElementById(\"kb-progress-bar9559_6c9af0-b4\");\n\t                                let elementAbove = document.getElementById(\"current-progress-above9559_6c9af0-b4\");\n\t                                let elementInside = document.getElementById(\"current-progress-inside9559_6c9af0-b4\");\n\t                                let elementBelow = document.getE